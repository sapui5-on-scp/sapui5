<mvc:View controllerName="ui5.test.controller.AppMain"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m"
	xmlns:t="sap.ui.table">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="">
					<content>
						<FlexBox height="12rem" alignItems="Center" justifyContent="Start" direction="Column">

							<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop" backgroundImage="./resources/ui5_logo.png" frameType="TwoByOne" press="onPressTile">
								<TileContent footer="클릭하시면 오픈채팅방으로 이동합니다." frameType="TwoByOne">
									<NewsContent contentText="#SAPUI5 #FIORI #SCP #Javascript #IT Trends" subheader="관심있으신 분은 참여 부탁드립니다. (67/100명)" />
								</TileContent>
							</GenericTile>
						</FlexBox>
						<Panel>
							<headerToolbar>
								<OverflowToolbar>
									<Title text="1. SAPUI5"/>
								</OverflowToolbar>
							</headerToolbar>
							<VBox id ="ui5VBox"/>
						</Panel>
						<Panel>
							<headerToolbar>
								<OverflowToolbar>
									<Title text="2. Javascript"/>
								</OverflowToolbar>
							</headerToolbar>
							<VBox id ="jsVBox"/>
						</Panel>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>




