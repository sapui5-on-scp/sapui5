	xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="">
					<content>
						<Panel>
							<FlexBox height="100%" alignItems="Center" justifyContent="Center">
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop" backgroundImage="./resources/ui5_logo.png" frameType="TwoByOne" press="onPressTile">
									<TileContent footer="클릭하시면 이동합니다." frameType="TwoByOne">
										<NewsContent contentText="#SAPUI5 #FIORI #SCP #Javascript #IT Trends" subheader="Kakao Talk Open Chatting" />
